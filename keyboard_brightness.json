{
    "description": "Adjust keyboard brightness (command + F1/F2)",
    "manipulators": [
        {
            "from": {
                "key_code": "f1",
                "modifiers": { "mandatory": ["left_command"] }
            },
            "to": [{ "key_code": "illumination_decrement" }],
            "type": "basic"
        },
        {
            "from": {
                "key_code": "f2",
                "modifiers": { "mandatory": ["left_command"] }
            },
            "to": [{ "key_code": "illumination_increment" }],
            "type": "basic"
        }
    ]
}
